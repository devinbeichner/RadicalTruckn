                <para LINESPMode="1"/>
                <ITEXT FONTSIZE="8" CH="100 Cards"/>
                <para LINESPMode="1"/>
                <ITEXT FONTSIZE="8" CH="42 Fuels"/>
                <para LINESPMode="1"/>
                <ITEXT FONTSIZE="8" CH="42 Tools"/>
                <para LINESPMode="1"/>
                <ITEXT FONTSIZE="8" CH="16 Drivers"/>
                <para LINESPMode="1"/>
                <ITEXT FONTSIZE="8" CH="4 Player Mats"/>
                <para LINESPMode="1"/>
                <para LINESPMode="1"/>
                <ITEXT FONTSIZE="8" CH="Player Mat:"/>
                <para LINESPMode="1"/>
                <ITEXT FONTSIZE="8" CH="Trailer - this is where the cargo you’re trying to ship goes."/>
                <para LINESPMode="1"/>
                <ITEXT FONTSIZE="8" CH="Reactor - Your fuel will go here."/>
                <para LINESPMode="1"/>
                <ITEXT FONTSIZE="8" CH="Driver’s Seat - This is where your driver goes"/>
